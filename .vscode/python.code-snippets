            "\tdef root(self,x):",
            "\t\tif self.parent[x]<0:return x",
            "\t\tself.parent[x]=self.root(self.parent[x])",
            "\t\treturn self.parent[x]",
            "\tdef merge(self,x,y):",
            "\t\trx=self.root(x)",
            "\t\try=self.root(y)",
            "\t\tif rx==ry:return",
            "\t\tif rx>ry:rx,ry=ry,rx",
            "\t\tself.parent[rx]+=self.parent[ry]",
            "\t\tself.parent[ry]=rx",
            "\tdef size(self,x):",
